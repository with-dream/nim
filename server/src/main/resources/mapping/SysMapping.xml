<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.server.mapper.SysMapper">

    <insert id="saveUuid" parameterType="long">
        insert into sys(uuid) values (#{uuid}});
    </insert>

    <select id="getUuid" resultType="long">
        SELECT uuid FROM sys;
    </select>

</mapper>


